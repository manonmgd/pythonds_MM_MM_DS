   "cell_type": "code",
   "execution_count": 87,
   "metadata": {},
   "outputs": [],
   "source": [
    "df_government['nombre_de_livres_deposes'] = pd.to_numeric(df_government['nombre_de_livres_deposes'], errors='coerce')"
