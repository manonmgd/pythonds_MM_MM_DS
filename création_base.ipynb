  {
   "cell_type": "code",
   "execution_count": 102,
   "metadata": {},
   "outputs": [],
   "source": [
    "# on fait la moyenne des taux pour importer une valeur dans la base de 2023\n",
    "moy_taux = taux.mean()\n",
    "df_government.loc[4, 'nombre_de_livres_deposes'] = int(df_government.loc[3, 'nombre_de_livres_deposes']*(1+moy_taux))"
   ]
